{
  "compilerOptions": {
    "target": "es2015",
    "module": "es2015",
    "outDir": "./dist",
    "removeComments": true,
    "sourceMap": true,
    "noEmit": false,
    "noEmitOnError": true,
    "strict": true,
    "alwaysStrict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true
  }
}
